        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.humorous.myapplication.application.TestApplication">
        <activity android:name="com.humorous.myapplication.MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>